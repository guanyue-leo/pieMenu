{
  "presets": ["stage-0", "es2015", "react", "env"],
  "env": {
    "development": {
      "plugins": [
        ["react-transform", {
        "transforms": [{
          "transform": "react-transform-hmr",
          "imports": ["react"],
          "locals": ["module"]
        }]
      }],
      ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": true }],
      "transform-decorators-legacy",
      "transform-class-properties"
      ]
    }
  }
}